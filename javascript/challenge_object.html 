<body>
	<script>
		var fruits = new Array();
  
        fruits[0] = 'apple';
        fruits[1] = 'grape';
        fruits[2] = 'melon';

		fruits.push('orange');

		for (var i = 0 ; i < fruits.length; i++) {
			document.write(fruits[i]);
		}
	</script>
</body>
